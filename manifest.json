{
  "manifest_version": 3,
  "name": "Subnect+",
  "version": "0.1.0",
  "version_name": "pre-Alpha 0.1.0",
  "description": "Enhance Your Subnect Experience",
  "icons": {
    "16": "icons/icon.svg",
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "content_scripts": [
    {
      "matches": ["https://subnect.net/*"],
      "js": ["content/main.js"],
      "css": ["styles/main.css"]
    }
  ],
  "background": {
    "service_worker": "background/main.js"
  },
  "permissions": [
    "storage"
  ]
}
